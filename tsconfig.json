{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./.nest",
    "baseUrl": "./",
    "strict": false,
    "incremental": true
  },
  "include": ["server/**/*", "client/**/*"],
  "exclude": ["node_modules",".nuxt-ssr",".nuxt-csr", ".nest"]
}
